{
  "name": "carecompanion",
  "version": "1.0.0",
  "private": true,
  "description": "AI-powered platform to help families manage care for aging parents with cognitive decline",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "test": "turbo run test",
    "lint": "turbo run lint",
    "db:generate": "turbo run db:generate",
    "db:migrate": "turbo run db:migrate",
    "db:studio": "turbo run db:studio",
    "clean": "turbo run clean && rm -rf node_modules",
    "seed:munson": "tsx scripts/run-seed.ts",
    "seed:cleanup": "tsx scripts/cleanup-seed.ts"
  },
  "devDependencies": {
    "turbo": "latest",
    "typescript": "^5.3.3",
    "@types/node": "^20.11.5",
    "prettier": "^3.2.4",
    "eslint": "^8.56.0",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "tsx": "^4.7.0"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "packageManager": "npm@10.2.4"
}
