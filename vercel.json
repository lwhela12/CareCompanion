{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "npx turbo run build --filter=@carecompanion/web...",
  "outputDirectory": "apps/web/dist",
  "framework": "vite",
  "installCommand": "npm install --include=dev --workspaces --include-workspace-root",
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
